[core]
    editor = subl
    excludesfile = ~/.gitignore
    pager = more
    quotepath = false

[alias]
    br = branch
    ci = commit
    co = checkout
    df = diff
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
    st = status
    subtree = !/usr/local/share/git-core/contrib/subtree/git-subtree.sh

